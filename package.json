{
  "name": "iqb-widgets",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test:project:verona-widget": "ng test verona-widget --watch false",
    "test:project:periodic-system-common": "ng test periodic-system-common --watch false",
    "test:project:periodic-system-select-widget": "ng test periodic-system-select-widget --watch false",
    "test:all": "run-p test:project:*",
    "watch:project:verona-widget": "ng build verona-widget --watch",
    "watch:project:periodic-system-common": "ng build periodic-system-common --watch",
    "watch:all": "run-p watch:project:*",
    "build:project:verona-widget": "ng build verona-widget",
    "build:project:periodic-system-common": "ng build periodic-system-common",
    "build:project:periodic-system-select-widget": "ng build periodic-system-select-widget",
    "build:all": "run-s build:project:verona-widget build:project:periodic-system-common build:project:periodic-system-select-widget",
    "gulp": "gulp",
    "gulp:project:periodic-system-select-widget": "gulp periodic-system-select-widget",
    "gulp:all": "run-p gulp:project:periodic-system-select-widget",
    "prettier": "prettier -w projects/verona-widget/src/ projects/periodic-system-common/src/ projects/periodic-system-select-widget/src/",
    "clean": "rimraf dist/"
  },
  "dependencies": {
    "@angular/cdk": "~20.2.12",
    "@angular/common": "^20.3.0",
    "@angular/compiler": "^20.3.0",
    "@angular/core": "^20.3.0",
    "@angular/forms": "^20.3.0",
    "@angular/material": "~20.2.12",
    "@angular/platform-browser": "^20.3.0",
    "@angular/router": "^20.3.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@angular/build": "^20.3.9",
    "@angular/cli": "^20.3.9",
    "@angular/compiler-cli": "^20.3.0",
    "@types/jasmine": "~5.1.0",
    "gulp": "^5.0.1",
    "gulp-inline": "^0.1.3",
    "jasmine-core": "~5.9.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ng-packagr": "^20.3.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.6.2",
    "rimraf": "^6.1.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "~5.9.2"
  }
}
